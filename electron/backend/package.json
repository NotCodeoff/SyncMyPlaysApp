{
  "name": "syncmyplays-backend",
  "version": "2.0.0",
  "description": "Modernized backend server for SyncMyPlays application",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "pkg index.js --targets node18-win-x64,node18-macos-x64,node18-linux-x64 --out-path ../dist-backend --assets data.json,server.cert --public-packages --public",
    "build:dev": "pkg src/server.js --targets node18-win-x64 --out-path ../dist-backend --assets data.json,server.cert,../scripts/puppeteer-config.js --public-packages --public"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^12.0.2",
    "electron-store": "^10.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fuse.js": "^7.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "p-limit": "^7.1.1",
    "pm2": "^5.3.0",
    "puppeteer": "^24.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "music",
    "playlist",
    "sync",
    "spotify",
    "apple-music",
    "api",
    "backend"
  ],
  "author": "SyncMyPlays Team",
  "license": "MIT"
}